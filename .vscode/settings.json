{
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [80],
  "editor.formatOnSave": false,
  "[javascript]": {
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.formatOnSave": true
  },
  "[typescriptreact]": {
    "editor.formatOnSave": true
  },
  "files.encoding": "utf8",
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "search.exclude": {
    "public/**": true,
    "node_modules/**": true
  },
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#cdf6ff",
    "activityBar.activeBorder": "#ff60e2",
    "activityBar.background": "#cdf6ff",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#ff60e2",
    "activityBarBadge.foreground": "#15202b",
    "statusBar.background": "#9aedff",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#67e4ff",
    "titleBar.activeBackground": "#9aedff",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#9aedff99",
    "titleBar.inactiveForeground": "#15202b99",
    "editorGroup.border": "#cdf6ff",
    "panel.border": "#cdf6ff",
    "sash.hoverBorder": "#cdf6ff",
    "sideBar.border": "#cdf6ff",
    "statusBarItem.remoteBackground": "#9aedff",
    "statusBarItem.remoteForeground": "#15202b"
  },
  "peacock.color": "#9aedff"
}
